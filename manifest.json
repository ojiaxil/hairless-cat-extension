{
    "manifest_version": 3,
    "name": "Hairless Cat Image Replacer",
    "version": "1.0",
    "description": "Replaces all images on every website with pictures of hairless cats.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["images/*"],
            "matches": ["<all_urls>"]
        }
    ]
}